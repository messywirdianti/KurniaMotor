<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@drawable/page_booking"
    tools:context=".BookingFragment">

    <!-- TODO: Update blank fragment layout -->


    <TextView
        android:id="@+id/booking"
        android:layout_width="244dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="90dp"
        android:layout_marginTop="100dp"
        android:layout_marginBottom="200dp"
        android:background="@drawable/rectangle"
        android:fontFamily="@font/sbpoppins"
        android:paddingStart="60dp"
        android:paddingTop="5dp"
        android:paddingEnd="45dp"
        android:paddingBottom="3dp"
        android:text="Booking Online"
        android:textColor="@color/white"
        android:textSize="16sp" />


    <TextView
        android:id="@+id/name_profile"
        android:layout_width="325dp"
        android:layout_height="95dp"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="250dp"
        android:background="@drawable/radius_circle"
        android:fontFamily="@font/rpoppins"
        android:paddingStart="20dp"
        android:paddingTop="5dp"
        android:text="Antrian No.01"
        android:textColor="@color/black"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nama : Mr.X"
        android:fontFamily="@font/rpoppins"
        android:textColor="@color/black"
        android:layout_marginLeft="70dp"
        android:layout_marginTop="250dp"
        android:paddingStart="20dp"
        android:paddingTop="25dp"
        android:textSize="11dp"/>

    <TextView
        android:id="@+id/merk"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Honda Scoopy Sporty Red"
        android:fontFamily="@font/rpoppins"
        android:textColor="@color/black"
        android:layout_marginLeft="70dp"
        android:layout_marginTop="250dp"
        android:paddingStart="20dp"
        android:paddingTop="45dp"
        android:textSize="11dp"/>

    <TextView
        android:id="@+id/plat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Plat : K 2023 LM"
        android:fontFamily="@font/rpoppins"
        android:textColor="@color/black"
        android:layout_marginLeft="70dp"
        android:layout_marginTop="250dp"
        android:paddingStart="20dp"
        android:paddingTop="65dp"
        android:textSize="11dp"/>

    <ImageView
        android:id="@+id/phoprof"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginLeft="275dp"
        android:layout_marginTop="265dp"
        android:src="@drawable/rectangle_51" />



</FrameLayout>